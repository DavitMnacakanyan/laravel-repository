{
    "name": "jetbox/laravel-repository",
    "description": "Laravel Repository Package",
    "keywords": [
        "laravel",
        "repository"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davit Mnacakanyan",
            "email": "davitmnacakanyan055@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0|^8.0|^8.1",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "JetBox\\Repositories\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "JetBox\\Repositories\\RepositoryServiceProvider"
            ],
            "aliases": {
                "JetBoxFile": "JetBox\\Repositories\\Support\\Facades\\JetBoxFile"
            }
        }
    }
}
